  "description": "Frontend for ontrack web app",
  "main": " ",
  "scripts": {
    "test": "karma start karma.conf.js",
    "move-index": "cp src/index.html dist/index.html",
    "build": "npm run move-index && webpack --config webpack.config.js",
    "start": "npm run move-index && concurrently \"webpack\" \"browser-sync start -s 'dist' --files 'dist' \"",
    "start:windows": "concurrently \"webpack\" \"browser-sync start -s dist --files dist \""
  },
